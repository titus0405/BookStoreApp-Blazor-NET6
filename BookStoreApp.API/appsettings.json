{
  "Serilog":
  {
    "MinimumLevel":
    {
      "Default": "Information",
      "Override":
      {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo":
    [
      {
        "Name": "File",
        "Args":
        {
          "path": "./Logs/log.text",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl":  "http://localhost:5341" } 
      }
    ]    
  },
  "AllowedHosts": "*"
}
